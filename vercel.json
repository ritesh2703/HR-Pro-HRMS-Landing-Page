{
  "version": 2,
  "routes": [
    {
      "src": "/assets/.*\\.(mp4|webm|mov)$",
      "headers": {
        "Content-Type": "video/mp4"
      },
      "continue": true
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/.*\\.(mp4|webm|mov)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}